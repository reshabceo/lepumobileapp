                                        sample_rate_hz: payload.sample_rate_hz,

                                        mv_per_count: payload.mv_per_count,

                                        duration_ms: payload.duration_ms,

                                        format: 'int16le',

                                        samples: waveformCounts.length,

                                    }), directory: Directory.ExternalStorage, encoding: Encoding.UTF8
